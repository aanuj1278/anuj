class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        //Write your code here
        long mx=0;
        long mn=Integer.MAX_VALUE;
        for(int i=0;i<n;i++){
            mx=Math.max(mx,a[i]);
            mn=Math.min(mn,a[i]);
        }
        pair pr=new pair(mn,mx);
        return pr;
    }
}
